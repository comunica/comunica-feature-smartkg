{
  "name": "@comunica/actor-init-sparql-smartkg",
  "version": "1.0.0",
  "description": "A Comunica engine for SPARQL query evaluation over SmartKG sources",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/actor-init-sparql-smartkg/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/actor-init-sparql-smartkg/^1.0.0/config/": "config/"
  },
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/comunica/comunica-feature-smartkg/tree/master/packages/actor-init-sparql-smartkg",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "actor",
    "init",
    "sparql",
    "smartkg"
  ],
  "license": "MIT",
  "bin": {
    "comunica-sparql-smartkg": "./bin/query.js",
    "comunica-sparql-smartkg-http": "./bin/http.js",
    "comunica-dynamic-sparql-smartkg": "./bin/query-dynamic.js"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://github.com/comunica/comunica#readme",
  "files": [
    "config",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.js",
    "index.d.ts",
    "index-browser.d.ts",
    "index-browser.js",
    "engine-default.js"
  ],
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.10.0",
    "@comunica/actor-query-operation-bgp-smartkg": "^1.0.0",
    "@comunica/actor-http-node-fetch": "^1.10.0",
    "@comunica/actor-rdf-resolve-quad-pattern-hdt": "^1.5.0",
    "@comunica/runner": "^1.9.4",
    "@comunica/runner-cli": "^1.9.4"
  },
  "scripts": {
    "lint": "node \"../../node_modules/tslint/bin/tslint\" lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "prepare": "comunica-compile-config config/config-default.json > engine-default.js"
  }
}
